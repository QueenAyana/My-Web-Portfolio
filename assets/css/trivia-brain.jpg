</script><script id="js-login-template" type="text/html">
    <div id="js-login-modal" class="modal chz-modal chz-modal-loginregister is-{{mode}}">
        <div class="modal-body chz-modal-loginregister-body">
            <span class="js-close close icons" data-icon="X"></span>
            <div>
                <span class="icons burger" data-icon="C"></span>
            </div>
            <div class="chz-modal-loginregister-panel col-xs-12">
                <a class="js-facebook-signin btn btn-fb-reg"><span class="facebook icons" data-icon="F"></span>Login with Facebook</a>
                <form>
                    {{#isRegisterMode}}
                    {{#facebookAccessToken}}
                    <p>Please fill out remaining information to register with your Facebook account.</p>
                    {{/facebookAccessToken}}
                    <div class="input-group">
                        <span class="input-group-addon">
                            <span class="icons" data-icon="0"></span>
                        </span>
                        <input name="email" type="email" placeholder="Email" required value="{{^facebookEmail}}{{email}}{{/facebookEmail}}{{facebookEmail}}"/>
                    </div>
                    <div class="input-group">
                        <span class="input-group-addon">
                            <span class="icons" data-icon="/"></span>
                        </span>
                        <input name="username" type="text" placeholder="{{resUsername}}" required pattern=".{6,50}" value="{{facebookUsername}}" title="{{resRegistrationUsernameTooltip}}"/>
                    </div>
                    {{/isRegisterMode}}
                    {{#isLoginMode}}
                    <div class="input-group">
                        <span class="input-group-addon">
                            <span class="icons" data-icon="/"></span>
                        </span>
                        <input name="username" placeholder="{{resUsernameOrEmail}}" type="text" required value=""/>
                    </div>
                    {{/isLoginMode}}
                    <div class="input-group">
                        <span class="input-group-addon">
                            <span class="icons" data-icon=""></span>
                        </span>
                        <input name="password" placeholder="{{resPassword}}" type="password" required pattern=".{6,}" value="" title="{{resRegistrationPasswordTooltip}}" autocomplete="off"/>
                    </div>
                    {{#errors.length}}
                    <div class="chz-alert chz-danger">
                        <h6 class="title">{{resRegistrationErrorListHeader}}</h6>
                        <ul>
                            {{#errors}}
                            <li>
                                <p>{{message}}</p>
                            </li>
                            {{/errors}}
                        </ul>
                    </div>
                    {{/errors.length}}

                    {{#isRegisterMode}}
                    <div class="checkbox">
                        <label for="legalConsent">
                            <input id="legalConsent" name="legalConsent" type="checkbox" checked="checked" required />
                            I accept the <a href="https://www.cheezburger.com/terms-of-service" rel="nofollow" target="_blank">Terms of Service</a> and <a href="https://www.cheezburger.com/privacy" rel="nofollow" target="_blank">Privacy Policy</a>
                        </label>
                    </div>
                    {{/isRegisterMode}}
                    <button class="btn btn-login text-center" type="submit" name="submit">{{btnCallToAction}}</button>

                    <div class="col-xs-6 nw-forgot-password">

                        {{#isLoginMode}}
                        <a href="{{forgotPasswordURL}}">{{resForgotYourPassword}}</a>
                        {{/isLoginMode}}
                        {{#shouldPromptToLoginOrReset}}
                        <span class="link js-reg-toggledialog">{{resLogin}}</span> {{resOr}} <a href="{{forgotPasswordURL}}">{{resMaybeShouldResetPassword}}</a>
                        {{/shouldPromptToLoginOrReset}}
                    </div>
                    <div class="col-xs-6">
                        {{footerMsg}}
                        <span class="link js-reg-toggledialog">
                            <a>{{footerCallToAction}}</a>
                        </span>
                    </div>
                </form>
            </div>

        </div>
    </div>
</script>    </div>







